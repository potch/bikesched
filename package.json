  },
  "devDependencies": {
    "autoprefixer": "^5.0.0",
    "browserify": "^9.0.3",
    "onchange": "0.0.2",
    "react-tools": "^0.12.2",
    "reactify": "^1.0.0"
  },
  "repository": {
    "type": "git",
